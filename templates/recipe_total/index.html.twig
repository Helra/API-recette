{% extends 'base.html.twig' %}

{% block title %}RecipeTotal index{% endblock %}

{% block body %}
    <h1>RecipeTotal index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for recipe_total in recipe_totals %}
            <tr>
                <td>{{ recipe_total.id }}</td>
                <td>
                    <a href="{{ path('recipe_total_show', {'id': recipe_total.id}) }}">show</a>
                    <a href="{{ path('recipe_total_edit', {'id': recipe_total.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('recipe_total_new') }}">Create new</a>
{% endblock %}
